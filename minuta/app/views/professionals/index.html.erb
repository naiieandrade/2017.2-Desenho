<p id="notice"><%= notice %></p>

<h1>Profissionais</h1>

<table>
  <thead>
    <tr>
      <th>Name professional</th>
      <th>Birth date</th>
      <th>Cpf</th>
      <th>Phone professional</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professionals.each do |professional| %>
      <tr>
        <td><%= professional.name_professional %></td>
        <td><%= professional.birth_date %></td>
        <td><%= professional.cpf %></td>
        <td><%= professional.phone_professional %></td>
        <td><%= link_to 'Show', professional %></td>
        <td><%= link_to 'Edit', edit_professional_path(professional) %></td>
        <td><%= link_to 'Destroy', professional, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professional', new_professional_path %>
